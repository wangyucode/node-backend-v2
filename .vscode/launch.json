{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "program": "${workspaceFolder}/src/index.js"
      },
      {
        "type": "node",
        "request": "attach",
        "name": "Attach to Process",
        "processId": "${command:pickProcess}",
        "skipFiles": [
          "<node_internals>/**"
        ]
      }
    ]
  }